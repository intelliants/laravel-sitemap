{
  "name": "intelliants/laravel-sitemap",
  "description": "Sitemap generation for Laravel",
  "keywords": ["sitemap", "laravel", "php"],
  "homepage": "https://github.com/intelliants/laravel-sitemap",
  "license": "MIT",
  "authors": [
    {
      "name": "Janur Jangaraev",
      "email": "janur.jangaraev@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/console": "^7.0|^8.0|^9.0",
    "illuminate/support": "^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.*|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Intelliants\\LaravelSitemap\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Intelliants\\LaravelSitemap\\LaravelSitemapServiceProvider"
      ],
      "aliases": {
        "LaravelSitemap": "Intelliants\\LaravelSitemap\\Facades\\LaravelSitemap"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}